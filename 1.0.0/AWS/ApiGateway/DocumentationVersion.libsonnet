{
  new(
    DocumentationVersion,
    RestApiId,
  ): {
    local base = self,
    Properties: {
      DocumentationVersion:
        if !std.isString(DocumentationVersion) then (error 'DocumentationVersion must be a string')
        else if std.isEmpty(DocumentationVersion) then (error 'DocumentationVersion must be not empty')
        else if std.length(DocumentationVersion) < 1 then error ('DocumentationVersion should have at least 1 characters')
        else DocumentationVersion,
      RestApiId:
        if !std.isString(RestApiId) then (error 'RestApiId must be a string')
        else if std.isEmpty(RestApiId) then (error 'RestApiId must be not empty')
        else if std.length(RestApiId) < 1 then error ('RestApiId should have at least 1 characters')
        else RestApiId,
    },
    DependsOn:: [],
    CreationPolicy:: [],
    DeletionPolicy:: [],
    UpdatePolicy:: [],
    UpdateReplacePolicy:: [],
    Metadata:: [],
    Type: 'AWS::ApiGateway::DocumentationVersion',
  },
  setDescription(Description): {
    Properties+::: {
      Description:
        if !std.isString(Description) then (error 'Description must be a string')
        else if std.isEmpty(Description) then (error 'Description must be not empty')
        else Description,
    },
  },
  setDependsOn(DependsOn): {
    Properties+::: {
      DependsOn: DependsOn,
    },
  },
  pushDependsOn(DependsOn): {
